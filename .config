server {
    listen 5000;
		server_name localhost;
	host 127.0.0.1;
    root server/;
	    index index.html;
	error_page 404 path/file/placeholder

    location / {
        allow_methods  DELETE POST GET;
        autoindex off;
    }

	location /cgi-bin {
        root ./placeholder;
        allow_methods GET POST DELETE;
        index time.py;
        cgi_path /usr/bin/python3 /bin/bash;
        cgi_ext .py .sh;
    }
}
